<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=
               "http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean name="tentedTruck"       class="project.domain.entity.pojo.truck.tented.TentedTruck"    scope="prototype"          />
    <bean name="tankTruck"         class="project.domain.entity.pojo.truck.tank.TankTruck"         scope="prototype"         />
    <bean name="refrigeratedTruck" class="project.domain.entity.pojo.truck.refrigerated.RefrigeratedTruck" scope="prototype" />


    <bean name="organization" class="project.domain.entity.pojo.organization.LogisticOrganization" scope="prototype"/>


    <bean name="box"     class="project.domain.entity.pojo.cargo.BoxCargo" scope="prototype" />
    <bean name="oil"     class="project.domain.entity.pojo.cargo.OilCargo"    scope="prototype"  />
    <bean name="product" class="project.domain.entity.pojo.cargo.ProductCargo" scope="prototype" />


    <bean name="driver"  class="project.domain.entity.pojo.employee.drivers.TruckDriver"   scope="prototype"   />
    <bean name="manager" class="project.domain.entity.pojo.employee.managers.LogisticManager" scope="prototype"/>

    
    <bean name="client" class="project.domain.entity.pojo.client.LogisticClient" scope="prototype"/>

</beans>